{{#each item}}
    <a class="item-top-cards text-normal-default" href="{{this.href}}"
       data-href="{{this.href}}">
        <img src={{this.img}} class="item-top-cards__image" data-href="{{this.href}}"
             alt={{this.key}}>
        <p id="mainpage_top-category" data-href="{{this.href}}">{{this.nameCategory}} </p>
    </a>
{{/each}}
